# Staging Environment
ENV=staging

# Supabase (Staging)
SUPABASE_URL=https://your-staging-project.supabase.co
SUPABASE_ANON_KEY=your-staging-anon-key

# API (Staging Server)
API_URL=https://staging-api.idol-support.com

# Stripe (Test Mode)
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_key

# Feature Flags
USE_MOCK=false
ENABLE_LOGGING=true
ENABLE_DEBUG_MODE=false

# Timeouts
API_TIMEOUT_SECONDS=20
MAX_RETRIES=3

# Cache Settings
CACHE_SIZE=100
CACHE_DURATION_MINUTES=15

# App Info
APP_VERSION=1.0.0
BUILD_NUMBER=1
